{
  "name": "kettlebell-tracker",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "A comprehensive fitness tracking PWA for kettlebell workouts, mobility, and yoga with offline support",
  "keywords": [
    "fitness",
    "workout",
    "kettlebell",
    "yoga",
    "mobility",
    "pwa",
    "tracker",
    "react",
    "typescript",
    "offline",
    "indexeddb",
    "charts",
    "audio"
  ],
  "author": "Fitness App Team",
  "homepage": "https://github.com/your-username/kettlebell-tracker",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/kettlebell-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/kettlebell-tracker/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "plop": "plop",
    "plop:component": "plop component",
    "plop:feature": "plop feature",
    "prepare": "husky",
    "analyze": "vite-bundle-analyzer dist",
    "serve": "vite preview --host",
    "clean": "rm -rf dist node_modules/.vite",
    "update-deps": "pnpm update --latest",
    "validate": "pnpm typecheck && pnpm lint && pnpm test"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.59.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dexie": "^4.0.8",
    "howler": "^2.2.4",
    "i18next": "^23.16.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "lucide-react": "^0.522.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.1.0",
    "react-router-dom": "^6.28.0",
    "recharts": "^2.12.7",
    "tailwind-merge": "^2.6.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "@tailwindcss/vite": "^4.0.0-beta.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/howler": "^2.2.11",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^2.1.5",
    "@vitest/ui": "^2.1.5",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^16.2.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "msw": "^2.6.4",
    "plop": "^4.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^4.0.0-beta.5",
    "tw-animate-css": "^1.3.4",
    "typescript": "~5.8.2",
    "vite": "^6.0.0",
    "vitest": "^2.1.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "pnpm@9.14.4",
  "license": "MIT",
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/your-username"
  },
  "contributors": [
    "Your Name <your.email@example.com>"
  ]
}